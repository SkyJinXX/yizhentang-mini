page {
  background: #f4f4f4;
}
.container {
  background-color: #fff;
}

.container-search {
  padding: 24rpx 30rpx;
}
  .search-box {
    display: flex;
    align-items: center;
    height: 64rpx;
    padding: 0 16rpx;
    border-radius: 17px;
    border: 1px solid #e6e6e6;

    font-size: 26rpx;
  }
    .search {
      width: -webkit-fill-available;
      margin-left: 10rpx;
    }

.container-menu {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  padding-bottom: 24rpx;
}
  .menu__xue {
    background: url(https://i.loli.net/2019/03/29/5c9e3474960e8.png) no-repeat center;
    background-size: contain;
    width: 100rpx;
    height: 288rpx;
  }
  .menu__mai {
    background: url(https://i.loli.net/2019/03/29/5c9e3474958e3.png) no-repeat center;
    background-size: contain;
    width: 100rpx;
    height: 288rpx;
  }
  .menu__hui {
    background: url(https://i.loli.net/2019/03/29/5c9e347495176.png) no-repeat center;
    background-size: contain;
    width: 100rpx;
    height: 287rpx;
  }
  .menu__xin {
    background: url(https://i.loli.net/2019/03/29/5c9e347496112.png) no-repeat center;
    background-size: contain;
    width: 100rpx;
    height: 286rpx;
  }

.container-swiper {
  display: flex;
  justify-content: center;
  align-items: center;

  width: 100%;
  height: 300rpx;

  margin-top: 10rpx;
}
.swiper-entry {
  width: 700rpx;
  height: 257rpx;
}
  
    .slide-image {
      width: 100%;
      height: 100%;
    }














